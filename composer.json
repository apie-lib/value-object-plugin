{
  "name": "apie/value-object-plugin",
  "description": "Composer package of the apie library: value object plugin",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Pieter Jordaan",
      "email": "pieter_jordaan@hotmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Apie\\ValueObjectPlugin\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Apie\\Tests\\ValueObjectPlugin\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.3",
    "apie/core": "dev-main",
    "apie/schema-generator": "dev-main",
    "symfony/serializer": "^4.3|^5.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "apie/composite-value-objects": "dev-main"
  },
  "suggest": {
    "apie/common-value-objects": "Collection of often used value objects"
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}